<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="description" content="Control panel for the WickedDevice temperature calibration chamber">
        <meta name="keywords" content="Control, Temperature, Humidity, Air Quality, Egg, Wicked Device">
        <meta name="author" content="Chris Rachlinski">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="application-name" content="TempChamberControl">
        <title>TempChamber Control Panel</title>
        <link rel="stylesheet" href="stylesheets/materialize.css">
        <link rel="stylesheet" href="stylesheets/style.css">
    </head>
    <body>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
        <script src="javascripts/ejs.js"></script>
        <script type="text/javascript" src="javascripts/materialize.js"></script>
        <script type="text/javascript" src="javascripts/sensordata.js"></script>
        
        
        <nav class="blue ">
            <div class="nav-wrapper">
                <a href="#" class="brand-logo margin-left">Wicked Device</a>
            </div>
        </nav>

        <div class="container">
            <div class="row" id="data-display">
            </div>

            <h1>Session Information (debug)</h1>
            <% if (session) { %>
            <p class="flow-text">Cookie:<%=JSON.stringify(session.cookie)%></p>
            <p class="flow-text">SessID:<%=session.id%></p>
            <p class="flow-text">Session:<%=JSON.stringify(session)%></p>
            <% }%>
        </div>
    </body>
</html>